{
  "name": "@dlm-daw/lib-box-forge",
  "version": "0.0.28",
  "private": true,
  "license": "LGPL-3.0-or-later",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "lint": "eslint \"**/*.ts\"",
    "clear": "rm -rf ./test/gen/*",
    "forge": "npx tsx test/schema.ts",
    "format": "prettier --write \"./test/gen/**/*.ts\" --ignore-path /dev/null",
    "gen": "npm run clear && npm run forge && npm run format",
    "test": "npm run gen && vitest run"
  },
  "dependencies": {
    "@dlm-daw/lib-box": "^0.0.28",
    "@dlm-daw/lib-dom": "^0.0.28",
    "@dlm-daw/lib-runtime": "^0.0.28",
    "@dlm-daw/lib-std": "^0.0.28",
    "ts-morph": "^25.0.1",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@dlm-daw/eslint-config": "^0.0.19",
    "@dlm-daw/typescript-config": "^0.0.20"
  }
}
